<?xml version="1.0" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="domain.Address" table="address">
        <id name="id" column="id">

            <generator class="identity"></generator>
        </id>
        <property name="address" type="java.lang.String" column="address"></property>
        <property name="city" type="java.lang.String" column="city"></property>
        <property name="county" type="java.lang.String" column="county"></property>
        <property name="secondaryAddress" type="java.lang.String" column="secondaryAddress"></property>
        <property name="secondaryCity" type="java.lang.String" column="secondaryCity"></property>
        <property name="secondaryCounty" type="java.lang.String" column="secondaryCounty"></property>

    </class>
</hibernate-mapping>