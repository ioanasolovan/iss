<?xml version="1.0" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="domain.Doctor" table="doctor">

        <id name="username" column="username">
            <generator class="assigned"/>
        </id>
        <property name="password"  type="java.lang.String" column="password" ></property>

        <property name="nume"  type="java.lang.String" column="nume" ></property>

        <property name="prenume"  type="java.lang.String" column="prenume" ></property>

        <property name="cnp"  type="java.lang.String" column="cnp" ></property>

    </class>
</hibernate-mapping>