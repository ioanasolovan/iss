<?xml version="1.0" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="domain.Patient" table="patient">
        <id name="id" column="id">
            <generator class="identity"></generator>
        </id>
        <property name="firstName" column="firstName" not-null="true"></property>
        <property name="lastName" column="lastName" not-null="true"></property>
        <property name="cnp" column="cnp" not-null="true"></property>
    </class>
</hibernate-mapping>